{
  "functions": {
    "api": {
      "timeout": 60,
      "runtime": "Nodejs16.13",
      "memorySize": 256,
      "installDependency": true,
      "triggers": [
        {
          "name": "httpTrigger",
          "type": "http",
          "config": {
            "path": "/*",
            "method": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
          }
        }
      ]
    }
  }
}